// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model users {
id          Int   @default(autoincrement()) @id
name        String
email       String @unique
password    String
cookieToken String
}

model music {
id                Int   @default(autoincrement()) @id
creatorToken      Int
name              String
composition       String
creationDate      Int
genre             String
}
